aws --endpoint-url=http://localhost:4566 secretsmanager create-secret \
    --name mysql-secret \
    --secret-string '{
        "url": "jdbc:mysql://localhost:3306/sso_db?createDatabaseIfNotExist=true&useSSL=false&serverTimezone=UTC",
        "username": "root",
        "password": "docker"
    }'
